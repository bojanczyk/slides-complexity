          {
            "type": "show",
            "id": "2:49",
            "name": "Maybe O(n3)? or O(n100)?",
            "index": 6,
            "disabled": false
          },
          {
            "type": "show",
            "id": "2:62",
            "name": "If we only care about polynomial time (or slower), then Turing Machines are a good model of computation.",
            "index": 7,
            "disabled": false
          },
          {
            "type": "show",
            "id": "2:64",
            "name": "Also true for logarithmic space.",
            "index": 8,
            "disabled": false
          }
        ]
