          {
            "type": "show",
            "id": "2:39",
            "name": "How much time does it take to sort a list of length n?",
            "index": 1,
            "disabled": false
          },
          {
            "type": "show",
            "id": "2:42",
            "name": "At least O(n) and at most O(n logn)",
